namespace com.sap.xs2.samples;

context AddressBook {

  @Catalog.tableType: #COLUMN
  Entity Book {
    key id : Integer; 
    name : String(100);
  };	
     
  @Catalog.tableType: #COLUMN
  Entity Address {
    key id     : Integer;
    book       : Association to Book;
    first_name : String(256);
    last_name  : String(256);
    address    : String(256);
    city       : String(256);
    country    : String(256);
    zip        : String(10);
    phone      : String(25);
    email      : String(50);
    web        : String(50);
  };	
     
};
