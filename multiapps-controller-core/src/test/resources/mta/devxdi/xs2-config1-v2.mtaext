_schema-version: "2.0.0"
ID: com.sap.devx.di.xs2-config1 
extends: com.sap.devx.di

targets: [XS2-INITIAL]

modules:
  - name: di-core
    parameters:
      port: 53030
      memory: 512M
      instances: 1
    properties:
      XS_PATH: /usr/sap/xs2runtime/bin/xs
      DI_FS_ROOT: /usr/sap/xs2work
    provides:
      - name: di-core-url
        properties:
          url: "http://sofd60245639a:53030"

  - name: di-builder
    parameters:
      port: 53031
      memory: 512M
      instances: 1
    properties:
      XS_PATH: /usr/sap/xs2runtime/bin/xs

  - name: di-runner
    parameters:
      port: 53033
      memory: 512M
      instances: 1
    properties:
      XS_PATH: /usr/sap/xs2runtime/bin/xs

resources:
 - name: ui5-content-url
   properties:
     url: "http://sofd60245639a:53055"
